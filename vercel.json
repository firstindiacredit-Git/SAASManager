{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'none'"
        }
      ]
    }
  ],
  "routes": [
    { 
      "src": "/[^.]+", 
      "dest": "/", 
      "status": 200 
    }
  ]
}